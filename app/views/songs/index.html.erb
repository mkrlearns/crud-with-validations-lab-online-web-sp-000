<table>
  <thead><h1>Song List</h1></thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.released %></td>
        <td><%= song.release_year %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
        <td><%= link_to 'View', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Delete', song, method: :delete, data: { confirm: 'Confirm Delete' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
